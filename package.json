{
  "name": "stencil",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build && yarn apply-hack && webpack",
    "apply-hack": "#",
    "build:cloud-bundler": "ts-node -T build/bundle.ts dist/wixCustomElement.server.js",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^1.3.3",
    "@stencil/router": "^1.0.1",
    "@vercel/node": "^1.11.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "@wix/cloud-bundler-core": "^2.0.11",
    "tmp": "^0.2.1",
    "ts-node": "latest"
  },
  "license": "MIT"
}
